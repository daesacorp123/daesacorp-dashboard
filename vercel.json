{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors * 'self' https://*.wordpress.com https://*.wp.com"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM *"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}